{
    "name": "spidgorny/nadlib",
    "require": {
		"php": ">=5.3.8",
		"michelf/php-markdown": "*@dev",
		"components/bootstrap": "dev-master@dev",
		"components/modernizr": "dev-master@dev",
		"mustangostang/spyc": "dev-master@dev",
		"flot/flot": "dev-master@dev",
		"php-sql-parser": "dev-master@dev",
		"GerHobbelt/HTMLawed": "dev-master@dev",
		"leafo/lessphp": "0.4.0@dev"
	},
	"require-dev": {
		"sebastian/environment": "1.0.*@dev",
		"phpunit/php-code-coverage": "2.1.*@dev",
		"phpunit/phpunit-mock-objects": "2.1.*@dev",
		"phpunit/phpunit": "dev-master"
    },
    "require_bak": {
		"pdepend/pdepend": "1.1.0",
		"fabpot/twittee": "dev-master",
		"sharom/twitter-bootstrap": "dev-master"
    },
	"autoload": {
		"_classmap": ["./", "be/", "Cache/", "Controller/", "CSS/", "Data/", "DB/", "Debug/", "HTML/", "HTMLForm/", "HTTP/", "LocalLang/", "ORM/", "SQL/", "Time/", "User/"],
		"_files": [
			"init.php"
		],
		"files": [
			"../../GerHobbelt/HTMLawed/htmLawed.php",
			"../../php-sql-parser/src/PHPSQLParser/PHPSQLParser.php",
			"../../php-sql-parser/src/PHPSQLParser/PHPSQLCreator.php",
			"../../mustangostang/spyc/Spyc.php"
		],
		"psr-0": {
			"htmLawed": "../../vendor/GerHobbelt/HTMLawed/",
			"PHPSQLParser": "../../php-sql-parser/src/PHPSQLParser/",
			"PHPSQLCreator": "../../php-sql-parser/src/PHPSQLParser/"
		}
	},
	"repositories_bak": [
		{
		"type": "vcs",
		"url": "https://github.com/michelf/php-markdown/"
		}
	],
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "michelf/php-markdown",
				"version": "master",
				"source": {
					"url": "https://github.com/michelf/php-markdown",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
            "type": "package",
            "package": {
                "name": "twitter/bootstrap",
				"version": "master",
				"source": {
                    "url": "https://github.com/twitter/bootstrap",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/twitter/bootstrap/zipball/master",
                    "type": "zip"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"thomas-mcdonald/bootstrap-sass",
                "source": {
                    "url":"https://github.com/thomas-mcdonald/bootstrap-sass.git",
                    "type":"git",
                    "reference":"master"
                },
                "dist": {
                    "url":"https://github.com/thomas-mcdonald/bootstrap-sass/zipball/master",
                    "type":"zip"
                }
            }
        },
		{
			"type": "vcs",
			"url": "https://github.com/flot/flot"
		},
        {
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"flot/flot",
                "source": {
                    "url":"https://github.com/flot/flot",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"mustangostang/spyc",
                "source": {
                    "url":"https://github.com/mustangostang/spyc",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"php-sql-parser",
                "source": {
                    "url":"http://php-sql-parser.googlecode.com/svn/",
                    "type":"svn",
                    "reference":"trunk"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"GerHobbelt/HTMLawed",
                "source": {
                    "url":"https://github.com/GerHobbelt/HTMLawed",
                    "type":"git",
                    "reference":"master"
                }
            }
        }
    ]
}
