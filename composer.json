{
	"name": "spidgorny/nadlib",
	"minimum-stability": "dev",
	"require": {
		"php": ">=5 || >=7"
	},
	"require-dev": {
		"phpunit/phpunit": "^6"
	},
	"suggest": {
		"flot/flot": "dev-master",
		"michelf/php-markdown": "~1.4",
		"components/bootstrap": "dev-master",
		"components/modernizr": "dev-master",
		"mustangostang/spyc": "dev-master",
		"greenlion/PHP-SQL-Parser": "*",
		"HTMLawed/HTMLawed": "dev-master",
		"leafo/lessphp": "0.4.0",
		"danielstjules/stringy": ">=1.5.1",
		"phpauth/phpauth": "dev-master",
		"gnat/nc-cms": "*",
		"robloach/component-installer": "^0.2.3",
		"hrmatching/advanced_html_dom": "dev-master",
        "squizlabs/php_codesniffer": "^3.0@dev"
	},
	"autoload": {
		"classmap": [
			"./",
			"Base/",
			"Cache/",
			"Controller/",
			"CSS/",
			"Data/",
			"DB/",
			"Debug/",
			"HTML/",
			"HTMLForm/",
			"HTTP/",
			"LocalLang/",
			"ORM/",
			"SQL/",
			"Queue/",
			"Runner/",
			"SQL/",
			"Time/",
			"User/"
		],
		"files": [
			"init.php"
		],
		"psr-0": {
			"": "src/"
		}
	},
	"autoload-dev": {
		"files": [
		]
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "michelf/php-markdown",
				"version": "master",
				"source": {
					"url": "https://github.com/michelf/php-markdown",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "gnat/nc-cms",
				"version": "master",
				"source": {
					"url": "https://github.com/gnat/nc-cms",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "twitter/bootstrap",
				"version": "master",
				"source": {
					"url": "https://github.com/twitter/bootstrap",
					"type": "git",
					"reference": "master"
				},
				"dist": {
					"url": "https://github.com/twitter/bootstrap/zipball/master",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"version": "dev-master",
				"name": "thomas-mcdonald/bootstrap-sass",
				"source": {
					"url": "https://github.com/thomas-mcdonald/bootstrap-sass.git",
					"type": "git",
					"reference": "master"
				},
				"dist": {
					"url": "https://github.com/thomas-mcdonald/bootstrap-sass/zipball/master",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"version": "dev-master",
				"name": "flot/flot",
				"source": {
					"url": "https://github.com/flot/flot",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"version": "dev-master",
				"name": "mustangostang/spyc",
				"source": {
					"url": "https://github.com/mustangostang/spyc",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"version": "dev-master",
				"name": "GerHobbelt/HTMLawed",
				"source": {
					"url": "https://github.com/GerHobbelt/HTMLawed",
					"type": "git",
					"reference": "master"
				}
			}
		}
	]
}
