{
    "name": "spidgorny/nadlib",
    "require": {
		"php": ">=5.3.8",
		"michelf/php-markdown": "*",
		"components/bootstrap": "dev-master",
		"components/modernizr": "dev-master"
	},
	"require-dev": {
		"phpunit/php-code-coverage": "1.4.*@dev",
		"sebastian/environment": "1.0.*@dev",
		"phpunit/phpunit-mock-objects": "1.4.*@dev",
		"phpunit/phpunit": "dev-master"
    },
    "require_bak": {
		"pdepend/pdepend": "1.1.0",
		"sharom/twitter-bootstrap": "dev-master"
    },
	"autoload": {
		"classmap": ["./", "be/", "Cache/", "Controller/", "CSS/", "Data/", "DB/", "Debug/", "HTML/", "HTMLForm/", "HTTP/", "LocalLang/", "ORM/", "SQL/", "Time/", "User/"],
		"files": ["init.php"]
	},
	"repositories_bak": [
		{
		"type": "vcs",
		"url": "https://github.com/michelf/php-markdown/"
		}
	],
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "michelf/php-markdown",
				"version": "master",
				"source": {
					"url": "https://github.com/michelf/php-markdown",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
            "type": "package",
            "package": {
                "name": "twitter/bootstrap",
				"version": "master",
				"source": {
                    "url": "https://github.com/twitter/bootstrap",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/twitter/bootstrap/zipball/master",
                    "type": "zip"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"thomas-mcdonald/bootstrap-sass",
                "source": {
                    "url":"https://github.com/thomas-mcdonald/bootstrap-sass.git",
                    "type":"git",
                    "reference":"master"
                },
                "dist": {
                    "url":"https://github.com/thomas-mcdonald/bootstrap-sass/zipball/master",
                    "type":"zip"
                }
            }
        }
    ]
}
